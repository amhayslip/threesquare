<%= form_for @location do |f| %>
<div class = "form">
  <%= f.label :name %>
  <%= f.text_field :name %>
</div>

<div class = "form">
  <%= f.label :category %>
  <%= f.text_field :category %>
</div>

<div class = "form">
  <%= f.label :neighborhood %>
  <%= f.text_field :neighborhood %>
</div>  

<div class = "form">
  <%= f.label :image_url %>
  <%= f.text_field :image_url %>
</div>  

<div class = "form">
  <%= f.label :review %>
  <%= f.text_field :review %>
</div>  

<div class = "form">
  <%= f.label :site_url %>
  <%= f.text_field :site_url %>
</div>

  <%= f.submit %>
<% end %>
